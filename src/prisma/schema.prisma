generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id String @id @default(uuid())
  email String  @unique
  name  String?
  password String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model vip {
  id String @id @default(uuid())
  name String
  description String
  photo String?
}

model gallery {
  id String @id @default(uuid())
  photo String?
}

model map {
  id String @id @default(uuid())
  photo String?
}

model emotions {
  id String @id @default(uuid())
  text String
  name String
  description String
  photo String?
}

model emotions_photo {
  id String @id @default(uuid())
  photo String?
}

model statistics {
  id String @id @default(uuid())
  number Int
  description String
}

model main {
  id String @id @default(uuid())
  photo String?
  description String
  about String
}

model main_photo {
  id String @id @default(uuid())
  photo String?
}

model routine {
  id String @id @default(uuid())
  name String
  file String?
}